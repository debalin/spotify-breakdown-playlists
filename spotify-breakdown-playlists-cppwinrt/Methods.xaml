<Page
	x:Class="spotify_breakdown_playlists_cppwinrt.Methods"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="using:spotify_breakdown_playlists_cppwinrt"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:muxc="using:Microsoft.UI.Xaml.Controls"
	mc:Ignorable="d">

	<muxc:NavigationView
		IsBackButtonVisible="Collapsed"
		IsPaneOpen="False"
		PaneTitle="">
		<muxc:NavigationView
			x:Name="NavView"
			IsBackButtonVisible="Collapsed"
			IsPaneOpen="False"
			PaneDisplayMode="Top"
			SelectionChanged="NavView_SelectionChanged">
			<muxc:NavigationView.MenuItems>
				<muxc:NavigationViewItem Content="Songs" Tag="songs" />
				<muxc:NavigationViewItemSeparator />
				<muxc:NavigationViewItem Content="Methods" Tag="methods" />
			</muxc:NavigationView.MenuItems>
			<ScrollViewer>
				<Frame
					x:Name="ContentFrame"
					Padding="12,0,12,24"
					IsTabStop="True"
					NavigationFailed="ContentFrame_NavigationFailed" />
			</ScrollViewer>
		</muxc:NavigationView>
	</muxc:NavigationView>
</Page>
